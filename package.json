{
  "name": "au-server",
  "version": "0.0.1",
  "description": "The attempt to create a REST API server using Aurelia DI and typestack",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./build/main.js",
    "start:watch": "npm run build:watch && nodemon ./build/main.js",
    "build": "rimraf ./build && node_modules/.bin/tsc",
    "build:watch": "npm run build -- -w",
    "test": "test"
  },
  "keywords": [
    "aurelia-dependency-injection"
  ],
  "author": "David Kossoglyad",
  "license": "MIT",
  "dependencies": {
    "aurelia-dependency-injection": "^1.5.2",
    "aurelia-metadata": "^1.0.7",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-polyfills": "^1.3.4",
    "body-parser": "^1.19.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "class-validator-jsonschema": "3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "multer": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.0",
    "typescript": "4.3.5"
  }
}
